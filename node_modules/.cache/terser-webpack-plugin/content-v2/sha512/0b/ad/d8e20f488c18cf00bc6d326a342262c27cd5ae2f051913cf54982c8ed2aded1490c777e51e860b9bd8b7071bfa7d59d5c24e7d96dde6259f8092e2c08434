}\n              onClick={switchAuthModeHandler}\n            >\n              {isLogin ? \"Create new account\" : \"Login with existing account\"}\n            </button>\n          )}\n          {!isLoading && <Link to=\"/reset\">Forgotten password?</Link>}\n        </div>\n      </form>\n    </section>\n  );\n};\n\nexport default AuthForm;\n","import AuthForm from '../components/Auth/AuthForm';\n\nconst AuthPage = () => {\n  return <AuthForm />;\n};\n\nexport default AuthPage;\n","import React from \"react\";\nimport \"./AnimationTwo.css\"\n\nconst AnimationTwo = () => {\n  return (\n    <div>\n      <div class=\"scene scene--hero\">\n        <div class=\"cube cube--hero is-spinning\">\n          <div class=\"cube__face cube__face--front\">Jollof Rice</div>\n          <div class=\"cube__face cube__face--right\">Gallo Pinto</div>\n          <div class=\"cube__face cube__face--back\">Masala Dosa</div>\n          <div class=\"cube__face cube__face--left\">Salad</div>\n          <div class=\"cube__face cube__face--top\">Fried Rice</div>\n          <div class=\"cube__face cube__face--bottom\">Plantain..</div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default AnimationTwo;\n","import React from \"react\";\nimport AnimationTwo from \"./AnimationTwo\";\nimport classes from \"./DetailedAnimation.module.css\";\n\nconst DetailedAnimation = () => {\n  return (\n    <div className={classes.container}>\n      <div className={classes.content}>\n        In-house technologies used to improve the functionality of nutrients in\n        your food, beverage and supplement products. Our functionally optimized\n        nutrient portfolio covers a wide variety of categories including\n        vitamins, minerals, botanicals, amino acids, sweeteners, carbohydrates,\n        fats (oils), fibers, and acids. NutraShield microencapsulated nutrients\n        have a protective coating called a matrix applied around the nutrients\n        core. The matrix keeps the active ingr